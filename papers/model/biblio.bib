	Date-Added = {2013-07-19 04:35:35 -0500},
	Date-Modified = {2013-10-02 23:58:33 +0000},
	Eprint = {1306.2023},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = jun,

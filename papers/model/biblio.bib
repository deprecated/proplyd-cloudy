	Date-Added = {2013-07-15 03:58:21 -0500},
	Date-Modified = {2013-10-02 23:55:08 +0000},
	Doi = {10.1111/j.1365-2966.2011.18681.x},
	Eprint = {1103.1705},
	Journal = {\mnras},
	Keywords = {atomic data, atomic processes, line: identification, planetary nebulae: individual: NGC 7009},
	Month = jul,
	Pages = {181-198},
	Primaryclass = {astro-ph.SR},
	Title = {{Very deep spectroscopy of the bright Saturn nebula NGC 7009 - I. Observations and plasma diagnostics}},
	Volume = 415,
	Year = 2011,

	Date-Modified = {2013-10-02 23:42:13 +0000},
	Doi = {10.1086/367954},
	Journal = {\apj},
	Keywords = {Galaxies: Individual: Messier Number: M33, Galaxies: Individual: Messier Number: M51, Galaxies: Individual: Messier Number: M81, Galaxies: Individual: NGC Number: NGC 5195, Galaxies: ISM, Galaxies: Spiral, ISM: Bubbles, ISM: Structure},

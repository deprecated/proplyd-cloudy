	Doi = {10.1111/j.1365-2966.2005.08810.x},
	Eprint = {arXiv:astro-ph/0501164},
	Journal = {\mnras},
	Keywords = {ISM: general, planetary nebulae: general},
	Month = apr,
	Pages = {457-467},
	Title = {{Helium recombination spectra as temperature diagnostics for planetary nebulae}},
	Volume = 358,
	Year = 2005,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2005.08810.x},

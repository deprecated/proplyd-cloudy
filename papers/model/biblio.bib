	Date-Modified = {2013-10-02 23:34:07 +0000},
	Doi = {10.1046/j.1365-8711.1998.01335.x},
	Journal = {\mnras},
	Keywords = {line: identification - ISM: abundances - H ii regions - ISM: individual: Orion nebula},
	Month = apr,
	Pages = {401},
	Title = {{Chemical composition of the Orion nebula derived from echelle spectrophotometry}},
	Volume = 295,

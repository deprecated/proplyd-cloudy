	Arxivurl = {http://arXiv.org/abs/astro-ph/0304011},
	Author = {{Zhang}, Y. and {Liu}, X.-W.},
	Date-Added = {2013-07-16 00:54:48 -0500},
	Date-Modified = {2013-10-02 23:42:51 +0000},
	Doi = {10.1051/0004-6361:20030466},
	Eprint = {arXiv:astro-ph/0304011},
	Journal = {\aap},
	Keywords = {ISM: abundances, planetary nebulae: individual: M 2-24, ISM: lines and bands},
	Month = jun,
	Pages = {545-556},
	Title = {{Optical spectrum of the planetary nebula M 2-24}},
	Volume = 404,
	Year = 2003,
	Bdsk-File-1 = {},

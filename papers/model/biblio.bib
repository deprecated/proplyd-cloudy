	Date-Added = {2013-01-16 11:28:47 -0600},
	Date-Modified = {2013-10-02 23:49:57 +0000},
	Doi = {10.1111/j.1365-2966.2008.13444.x},
	Eprint = {0805.1362},
	Journal = {\mnras},
	Keywords = {stars: atmospheres , stars: early-type , stars: fundamental parameters , HII regions},
	Month = sep,
	Pages = {1009-1021},
	Title = {{The ionizing radiation from massive stars and its impact on HII regions: results from modern model atmospheres}},
	Volume = 389,

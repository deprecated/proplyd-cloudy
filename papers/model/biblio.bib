	Journal = {\aj},
	Keywords = {GALAXIES: INDIVIDUAL: NGC 55, GALAXIES: INDIVIDUAL: NGC 253, GALAXIES: INDIVIDUAL: NGC 300, GALAXIES: ISM, HII REGIONS},
	Month = oct,

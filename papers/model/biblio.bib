	Keywords = {atomic data, line: formation, Hii regions, ISM: atoms, planetary nebulae: general},
	Month = jun,
	Pages = {A18},
	Primaryclass = {astro-ph.SR},
	Title = {{New effective recombination coefficients for nebular N ii lines}},
	Volume = 530,
	Year = 2011,

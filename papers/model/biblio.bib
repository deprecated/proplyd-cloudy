	Date-Modified = {2013-10-02 23:58:43 +0000},
	Eprint = {1306.6110},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics, Astrophysics - Cosmology and Extragalactic Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	Month = jun,

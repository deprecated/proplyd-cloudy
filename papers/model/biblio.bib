	Doi = {10.1086/300178},
	Journal = {\aj},
	Keywords = {H II REGIONS, INTERSTELLAR MATTER, STAR FORMATION, ORION NEBULA, PHOTOIONIZATION, CALIBRATING, LYMAN ALPHA RADIATION},
	Month = jan,
	Pages = {263},
	Title = {{Observational properties of the Orion Nebula proplyds}},
	Volume = 115,
	Year = 1998,
